{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "currency": {
        "type": "string",
        "enum": ["RUR", "USD"]
      },
      "balance": {
        "type": "number"
      }
    },
    "required": ["id", "currency", "balance"]
  }
}
